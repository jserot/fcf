type 'a option2 = None | Some of 'a * 'a;

let foo (a:int option2) = 
  let f (x) =
  | x~None -> return 0 
  | x~Some (1,1) -> return 1
  | x~Some (2,0) -> return 2
  | x~Some (u,0) -> return u*10
  | x~Some (u,v) -> return u*v in
  f (a)
;

foo(None);
foo(Some (1,1));
foo(Some (2,0));
foo(Some (3,0));
foo(Some (4,100));
